tailwind.config.js
postcss.config.js

# Development files
src/
components/
lib/
*.md
.env.local
.env.development

# IDE
.vscode/
.idea/

# Tests
__tests__/
*.test.js
*.spec.js

# Misc
.DS_Store
*.log